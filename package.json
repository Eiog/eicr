{
  "name": "eicr",
  "type": "module",
  "version": "0.0.1-beta.1",
  "packageManager": "pnpm@7.18.0",
  "description": "Eicr-多项目快速开始模板 by<https://github.com/Eiog>",
  "author": "Eiog",
  "license": "MIT",
  "homepage": "https://github.com/Eiog/eicr",
  "repository": {
    "type": "git",
    "url": "git@github.com:Eiog/eicr.git"
  },
  "bugs": "https://github.com/Eiog/eicr/issues",
  "keywords": [
    "eicr"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "eicr": "./dist/index.js"
  },
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "zr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "update-deps": "taze -w && pnpm i",
    "typecheck": "tsc --noEmit",
    "eicr": "eicr"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.36.0",
    "@types/fs-extra": "^11.0.1",
    "@types/inquirer": "^9.0.3",
    "@types/node": "^18.15.0",
    "bumpp": "^9.0.0",
    "eslint": "^8.36.0",
    "esno": "^0.16.3",
    "glob": "^9.2.1",
    "taze": "^0.9.0",
    "tsup": "6.6.3",
    "typescript": "^4.9.5",
    "vitest": "^0.29.2"
  },
  "eslintConfig": {
    "extends": "@antfu"
  },
  "dependencies": {
    "chalk": "^5.2.0",
    "clear-console": "^1.1.0",
    "commander": "^10.0.0",
    "download-git-repo": "^3.0.2",
    "fs-extra": "^11.1.0",
    "inquirer": "^9.1.4",
    "ora": "^6.1.2",
    "shelljs": "^0.8.5"
  }
}
